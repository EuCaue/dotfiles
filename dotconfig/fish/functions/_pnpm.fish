function set_pnpm_aliases
    alias pnpms='pnpm start'
    alias pnpmb='pnpm build'
    alias pnpmpb='pnpm preview:build'
    alias pnpmd="pnpm dev"
    alias pnpmt="pnpm test"
    alias pnpmp="pnpm prune"
    alias pnpmpv="pnpm preview"
    alias pnpxv="pnpx vercel"
    alias pnpxvp="pnpx vercel --prod"
end

set_pnpm_aliases
